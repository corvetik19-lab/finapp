# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# OpenAI API (для embeddings, OCR и другие задачи)
# Получите ключ на: https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-xxxxxxxxxxxxx

# OpenRouter API (для AI Студии - 400+ моделей)
# Получите ключ на: https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-v1-xxxxxxxxxxxxx
OPENROUTER_SITE_URL=https://your-app.vercel.app
OPENROUTER_SITE_NAME=Finapp AI Studio

# OpenRouter API для режима Финансы (отдельный ключ)
# Используется для AI чата, embeddings, OCR в финансовом режиме
# Модели: google/gemini-2.5-flash-preview-05-20, openai/text-embedding-3-large
OPENROUTER_FINANCE_API_KEY=sk-or-v1-xxxxxxxxxxxxx

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Site Info (опционально, для рейтинга на openrouter.ai)
NEXT_PUBLIC_SITE_URL=https://your-app.vercel.app

# Resend Email Service
# Получите ключ на: https://resend.com/api-keys
RESEND_API_KEY=your_resend_api_key
RESEND_FROM_EMAIL=Finapp <noreply@yourdomain.com>

# App URL для ссылок в email
NEXT_PUBLIC_APP_URL=http://localhost:3000
